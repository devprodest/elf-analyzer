{
  "name": "devprodest-elf-analyzer",
  "displayName": "ELF size analyzer",
  "description": "ELF analyzer. List and size of sections and elements.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/devprodest/devprodest-elf-analyzer"
  },
  "bugs": {
    "url": "https://github.com/devprodest/devprodest-elf-analyzer/issues"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [],
    "configuration": [
      {
        "title": "ELF size analyzer",
        "properties": {
          "devprodest-elf-analyzer.llvm-bin-path": {
            "type": "string",
            "description": "Path to llvm bin directory"
          }
        }
      }
    ],
    "customEditors": [
      {
        "viewType": "devprodest-elf-analyzer.elf-view",
        "displayName": "ELF size analyzer",
        "selector": [
          {
            "filenamePattern": "*.elf"
          }
        ]
      }
    ]
  },
  "scripts": {
    "compile": "npm run check-types && npm run lint && node esbuild.js --production",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",

    "vscode:prepublish": "npm run package",
    "package": "node esbuild.js --production",
    "vscode:build": "vsce package"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2"
  }
}